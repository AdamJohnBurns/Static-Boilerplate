{

    // Enforcing options
    // http://www.jshint.com/docs/options/#enforcing-options

    "bitwise": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "futurehostile": true,
    "latedef": true,
    "maxstatements": 12,
    "noarg": true,
    "nonbsp": true,
    "nonew": true,
    "strict": true,
    "undef": true,
    "unused": true,

    // - - - - - - - - - - - - - - - - - - - - - - - - - - -

    // Relaxing options
    // http://www.jshint.com/docs/options/#relaxing-options

    //"esnext": true,

    // - - - - - - - - - - - - - - - - - - - - - - - - - - -

    // Environments
    // http://www.jshint.com/docs/options/#environments

    "browser": true,
    "devel": true,

    // - - - - - - - - - - - - - - - - - - - - - - - - - - -

    // Globals
    "globals": {
        "App": true
    }
}
